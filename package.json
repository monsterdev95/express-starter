{
  "name": "express-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node ./dist/bin/www.js",
    "start:dev": "nodemon --exec babel-node ./src/bin/www.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1"
